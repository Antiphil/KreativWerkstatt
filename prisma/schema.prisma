// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Post {
  id        String   @id
  published Boolean @default(true)
  title     String
  artists   String
  createdAt DateTime @default(now())
  imageid   String
  imageid2  String?
  imageid3  String?
  imageid4  String?
}
